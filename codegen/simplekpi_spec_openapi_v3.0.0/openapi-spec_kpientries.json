    "servers":[
        {
            "url":"https://YOURDOMAIN.simplekpi.com/api"
        }
    ],
    "security":[
        {
            "basicAuth":[

            ]
        }
    ],
    "components":{
        "securitySchemes":{
            "basicAuth":{
                "type":"http",
                "scheme":"basic"
            }
        },
        "schemas":{
            "KPIEntry":{
                "required":[
                    "user_id",
                    "kpi_id",
                    "entry_date"
                ],
                "properties":{
                    "id":{
                        "type":"integer",
                        "format":"int64",
                        "description":"Automatically generated for the KPI entry"
                    },
                    "user_id":{
                        "type":"integer",
                        "format":"int64",
                        "description":"An id of an active user to assign to the KPI entry"
                    },
                    "kpi_id":{
                        "type":"integer",
                        "format":"int64",
                        "description":"The kpi must be active and cannot be a calculated KPI. The KPI must also be assigned to the user"
                    },
                    "entry_date":{
                        "type":"string",
                        "description":"The date of the entry. Date time format without timezone, e.g. `2019-01-01T00:00:00`"
                    },
                    "actual":{
                        "type":"number",
                        "format":"double",
                        "description":"The actual value cannot be null if the target and notes are both null"
                    },
                    "target":{
                        "type":"number",
                        "format":"double",
                        "description":"The target value of the entry. This value will be ignored if the KPI has a null target"
                    },
                    "notes":{
                        "type":"string",
                        "description":"The note associated with the KPI entry",
                        "maxLength":500
                    },
                    "created_at":{
                        "type":"string",
                        "description":"The UTC date and time the KPI entry was created. Date time format without timezone, e.g. `2019-01-01T00:00:00`"
                    },
                    "updated_at":{
                        "type":"string",
                        "description":"The UTC date and time the KPI entry was updated. Date time format without timezone, e.g. `2019-01-01T00:00:00`"
                    }
                }
            }
        }
    }
